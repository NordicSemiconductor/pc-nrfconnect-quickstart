    "verify": {
        "type": "AT",
        "commands": [
            {
                "title": "Manufacturer",
                "command": "AT+CGMI",
                "responseRegex": "(.*)",
                "copiable": false
            },
            {
                "title": "Hardware Version",
                "command": "AT%HWVERSION",
                "responseRegex": "%HWVERSION: (.*)",
                "copiable": false
            },
            {
                "title": "International Mobile Equipment Identity",
                "command": "AT+CGSN=1",
                "responseRegex": "\\+CGSN: \"(.*)\"",
                "copiable": true
            }
        ]
    },
